{
	"name": "ksl",
	"version": "1.0.0",
	"description": "Keystroke launcher for developers who loves JS",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lis355/ksl.git"
	},
	"main": "start.js",
	"build": {
		"productName": "ksl",
		"artifactName": "${productName}.${ext}",
		"asar": false,
		"appId": "com.ksl.app",
		"compression": "maximum",
		"win": {},
		"directories": {
			"buildResources": "buildfiles",
			"app": "buildfiles",
			"output": "build"
		}
	},
	"dependencies": {
		"electron": "^11.2.1",
		"electron-better-ipc": "^1.1.1",
		"lowdb": "^1.0.0",
		"ndapp": "^1.0.16",
		"node-powershell": "^4.0.0"
	},
	"devDependencies": {
		"electron-builder": "^22.9.1",
		"eslint": "^7.18.0",
		"eslint-config-protop": "^1.0.0",
		"shell-quote": "^1.7.2",
		"webpack": "^5.21.2",
		"webpack-cli": "^4.5.0"
	}
}
