{
	"name": "ksl",
	"version": "2.0.0",
	"description": "Keystroke launcher for developers who loves JS",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lis355/ksl.git"
	},
	"type": "module",
	"main": "start.js",
	"build": {
		"productName": "ksl",
		"artifactName": "${productName}.${ext}",
		"asar": true,
		"win": {
			"target": [
				"portable"
			]
		},
		"directories": {
			"buildResources": "buildfiles",
			"app": "buildfiles",
			"output": "build"
		}
	},
	"dependencies": {
		"dayjs": "^1.11.13",
		"electron": "^33.0.2",
		"lodash": "^4.17.21",
		"mathjs": "^13.2.0",
		"node-powershell": "^5.0.1"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.23.3",
		"electron-builder": "^25.1.8",
		"eslint": "^8.54.0",
		"shell-quote": "^1.8.1",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4"
	}
}