{
	"name": "ksl",
	"version": "2.1.2",
	"description": "Keystroke launcher for developers who loves JS",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lis355/ksl.git"
	},
	"type": "module",
	"main": "start.js",
	"scripts": {
		"app-build": "yarn run webpack --config webpack.config.js"
	},
	"build": {
		"productName": "ksl",
		"artifactName": "${productName}-${arch}-${platform}-${version}.${ext}",
		"asar": false,
		"win": {
			"icon": "assets/icon.ico",
			"target": "portable"
		},
		"directories": {
			"buildResources": "buildfiles",
			"app": "buildfiles",
			"output": "build"
		}
	},
	"dependencies": {
		"@lis355/enumjs": "^1.0.1",
		"ansi-escape-sequences": "^6.2.3",
		"dayjs": "^1.11.13",
		"electron": "^33.0.2",
		"lodash": "^4.17.21",
		"mathjs": "^13.2.0",
		"node-powershell": "^5.0.1",
		"stack-trace": "^1.0.0-pre2"
	},
	"devDependencies": {
		"electron-builder": "^25.1.8",
		"shell-quote": "^1.8.1",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4"
	}
}